{
  "name": "game-db",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gen-changelog": "npm exec git-cliff > CHANGELOG.md",
    "chgl-commit-push": "git add CHANGELOG.md && git commit -m 'Updated changelog' && git push origin $(git rev-parse --abbrev-ref HEAD)",
    "chgl-update": "npm run gen-changelog && npm run chgl-commit-push",
    "dev:ssr": "ng run GameDB:serve-ssr",
    "serve:ssr": "node dist/GameDB/server/main.js",
    "build:ssr": "ng build && ng run GameDB:server",
    "prerender": "ng run GameDB:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/platform-server": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/service-worker": "^17.0.8",
    "@angular/ssr": "^17.0.8",
    "express": "^4.18.2",
    "flowbite": "^2.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/cli": "~17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.18.0",
    "autoprefixer": "^10.4.16",
    "git-cliff": "^1.4.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.2.2"
  }
}
